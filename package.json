{
  "name": "@ludovicm67/sparql-datasource",
  "version": "0.0.0",
  "private": true,
  "description": "Add support for SPARQL",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "release": "changeset tag"
  },
  "author": {
    "name": "Ludovic Muller",
    "url": "https://ludovic-muller.fr/"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@changesets/cli": "^2.27.1",
    "@grafana/e2e": "10.3.3",
    "@grafana/e2e-selectors": "10.3.3",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.4.2",
    "@swc/helpers": "^0.5.6",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.2.5",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.71.1",
    "sass-loader": "14.1.1",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "10.3.3",
    "@grafana/runtime": "10.3.3",
    "@grafana/ui": "10.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.6.2"
  },
  "packageManager": "npm@10.1.0"
}
